3d_render/layer_2="counters"
3d_render/layer_3="walls"
2d_physics/layer_1="Player"
3d_physics/layer_1="player"
2d_physics/layer_2="PhantomCameraArea"
3d_physics/layer_2="walls"
3d_physics/layer_3="counters"
